{
    "name": "lesi-electron",
    "version": "1.0.0",
    "main": "./src/electron/main.js",
    "description": "",
    "scripts": {
        "dev": "concurrently \"vite\" \"cross-env NODE_ENV=development electronmon src/electron/main.js\"",
        "build": "vite build",
        "clean": "node scripts/clean.js",
        "build:setup": "npm run setup:playwright && npm run clean && npm run build",
        "build:win": "npm run build:setup && electron-builder --win",
        "build:mac": "npm run build:setup && electron-builder --mac",
        "build:linux": "npm run build:setup && electron-builder --linux",
        "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
        "type-check": "tsc -b",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx}\""
    },
    "keywords": [
        "electron"
    ],
    "author": "",
    "license": "ISC",
    "type": "commonjs",
    "dependencies": {
        "@radix-ui/react-slot": "^1.2.3",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "dotenv": "^17.2.0",
        "electron-updater": "^6.6.2",
        "framer-motion": "^12.23.6",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-router-dom": "^7.6.3",
        "tailwind-merge": "^3.3.1",
        "tailwindcss": "^3.4.1",
        "wait-on": "^8.0.3"
    },
    "devDependencies": {
        "@eslint/js": "^9.31.0",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "@typescript-eslint/eslint-plugin": "^8.37.0",
        "@typescript-eslint/parser": "^8.37.0",
        "@vitejs/plugin-react": "^4.6.0",
        "autoprefixer": "^10.4.21",
        "concurrently": "^9.2.0",
        "cross-env": "^7.0.3",
        "electron": "^37.2.0",
        "electron-builder": "^26.0.12",
        "electronmon": "^2.0.3",
        "eslint": "^9.31.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-react": "^7.37.5",
        "postcss": "^8.5.6",
        "prettier": "^3.6.2",
        "rimraf": "^6.0.1",
        "terser": "^5.43.1",
        "typescript": "^5.8.3",
        "vite": "^7.0.2"
    }
}
